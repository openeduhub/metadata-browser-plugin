{
  "manifest_version": 2,

  "name": "edu-sharing",
  "description": "",
  "version": "1.0",
  "options_page": "options.html",
  "icons": {
    "16": "edu16.png",
    "48": "edu48.png",
    "128": "edu128.png" },
  "browser_action": {
    "default_icon": "edu48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "notifications",
    "storage",
    "*://*/*"
  ]
}
