{
  "manifest_version": 3,
  "name": "edu-sharing",
  "description": "",
  "version": "1.0",
  "default_locale": "de",
  "options_page": "options.html",
  "action":{
    "default_popup": "metadata_form.html",
    "default_title": "Metadaten erfassen"
  },
  "icons": {
    "16": "edu16.png",
    "48": "edu48.png",
    "128": "edu128.png" 
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "contextMenus",
    "notifications",
    "storage"
  ]
}
