{
  "manifest_version": 3,

  "name": "edu-sharing",
  "description": "",
  "version": "1.0",
  "options_page": "options.html",
  "action":{
    "default_popup": "metadata_form.html",
    "default_title": "Options test"
  },
  "icons": {
    "16": "edu16.png",
    "48": "edu48.png",
    "128": "edu128.png" },
  "browser_action": {
    "default_icon": "edu48.png",
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "notifications",
    "storage",
    "*://*/*"
  ]
}
